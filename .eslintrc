{
  "extends": "airbnb-base",
  "plugins": [ "eslint-plugin-meteor" ],
  "rules": {
    "semi": 0,
    "no-param-reassign": [2, { "props": false }],
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "new-cap": [2, {
      "capIsNewExceptions": [
        "Match.Optional",
        "Match.OneOf"
      ]
    }]
  }
}
